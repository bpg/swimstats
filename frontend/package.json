    "eslint": "9.39.2",
    "eslint-plugin-react-hooks": "7.0.1",
    "eslint-plugin-react-refresh": "0.4.26",
    "globals": "17.0.0",
    "jsdom": "27.4.0",
    "msw": "2.12.7",
    "postcss": "8.5.6",
    "prettier": "3.8.0",
    "tailwindcss": "4.1.18",
    "typescript": "5.9.3",
    "typescript-eslint": "8.53.1",
    "vite": "7.3.1",
    "vitest": "4.0.17",
    "vitest-axe": "0.1.0"
